<% @properties.each do |property| %>
    <% if property.images.attached? %>
        <% property.images.each do |image| %>
            <%= image_tag(image, style: "width: auto; height: 250px")%>
        <% end %>
    <% end %>
    <%= link_to property.name, property, class: "btn btn-primary"  %>
<% end %>
